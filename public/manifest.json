{
  "manifest_version": 2,
  "name": "V Rising Server Scanner",
  "author": "Tony Pettigrew",
  "version": "1.0.1",
  "description": "Scan V Rising dedicated servers and view information about them",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon-16.ico",
      "48": "favicon-48.ico",
      "128": "favicon-128.ico"
    }
  },
  "icons": {
    "16": "favicon-16.ico",
    "48": "favicon-48.ico",
    "128": "favicon-128.ico"
  }
}